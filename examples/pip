
- name: install buiagent pip packages
  pip:
    name: "{{ item }}"
    state: present
  with_items: "{{ burp_agent_pip_present }}"

- name: Install buiagent pip
  pip:
    name: "{{ item.name }}"
    state: present
    version: "{{ item.version}}"
  with_items: "{{ burpui_pip_packages }}"
  notify: restart buiagent